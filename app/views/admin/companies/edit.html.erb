<% @title = '加盟店情報の編集' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for [ :admin, @company ] do |f| %>
    <%= render 'form', f: f %>
    <div class="buttons" style="text-align:center;">
      <%= f.submit '更新',id:"edit_submit" %>
      <%= link_to 'キャンセル', :admin_companies,id:"edit_cancel" %>
    </div>
  <% end %>
</div>
