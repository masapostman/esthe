<% @title = 'アカウント情報編集' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for @staff_member, url: :staff_account do |f| %>
    <%= render 'form', f: f %>
    <div class="buttons" style="text-align:center;">
      <%= f.submit '更新',id:"edit_submit" %>
      <%= link_to 'キャンセル', :staff_account ,id:"edit_cancel" %>
    </div>
  <% end %>
</div>
